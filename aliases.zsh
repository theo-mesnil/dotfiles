alias aliases="code ~/.aliases"
alias gitconfig="code ~/.gitconfig"
alias hosts="sudo code /etc/hosts"

alias be="bundle exec"

alias nginx-stop="sudo nginx -s stop"
alias nginx-start="sudo nginx"
alias nginx-restart="sudo nginx -s reload"
alias sites-available="cd /usr/local/etc/nginx/sites-available"
alias sites-enabled="cd /usr/local/etc/nginx/sites-enabled"
alias startWk="procodile start --dev"
alias postgres-start="~/.asdf/installs/postgres/11.1/bin/pg_ctl -D ~/.asdf/installs/postgres/11.1/data -l logfile start"
